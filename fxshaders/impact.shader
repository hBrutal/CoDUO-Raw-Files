//=======scorch generic============//


impact/scorch_gen_a
{
	entityMergable
	surfaceparm nonsolid
	surfaceparm trans
	polygonOffset2
    {
       map gfx/efx_assets/impacts/scorch_gen_a.dds
 	blendfunc blend
        rgbGen vertex
    nextbundle
		map $lightmap
    }
    {
		perlight
        map gfx/efx_assets/impacts/scorch_gen_a.dds
        blendfunc GL_SRC_ALPHA GL_ONE
        rgbGen vertex
    nextbundle
		map $dlight
    }
}


