textures/ardennes/transparents/foy_field_fence
{
	surfaceparm metal
	surfaceparm trans
	surfaceparm alphashadow
	surfaceparm nomarks
	surfaceparm nonsolid
	cull none
	nomipmaps
	nopicmip
	qer_editorimage textures/ardennes/misc/metal_masked@barbfence2.tga
	{
		map textures/ardennes/misc/metal_masked@barbfence2
		rgbGen exactVertex
		alphaFunc GE128
		depthWrite
	nextbundle
		map $lightmap
	}
}

