	polygonOffset
	qer_editorimage textures/ardennes/terrain/snow@dirtysnow1.tga
	{
		map textures/ardennes/terrain/snow@dirtysnow1
		rgbGen exactVertex
		alphagen vertex
		blendFunc blend
	nextbundle
